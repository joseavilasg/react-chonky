{
  "name": "@bhunter179/chonky",
  "version": "0.2.46",
  "description": "A File Browser component for React",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "vite": "vite ./example",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "release": "npm publish --access=public",
    "patch": "npm --no-git-tag-version version patch",
    "format": "prettier --write '**/*.{ts,tsx}'",
    "format-check": "prettier --check '**/*.{ts,tsx}",
    "update": "npx --yes npm-check-updates -u && npm install"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "treeshake": true,
    "sourcemap": "inline",
    "splitting": false,
    "format": "esm",
    "dts": true,
    "external": [
      "react"
    ]
  },
  "peerDependencies": {
    "react": ">=18",
    "@mui/material": ">=5"
  },
  "dependencies": {
    "@bhunter179/react-material-you-theme": "^0.1.23",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@reduxjs/toolkit": "^2.0.1",
    "deepmerge": "^4.3.1",
    "exact-trie": "^1.0.13",
    "fast-sort": "^3.4.0",
    "filesize": "^10.1.0",
    "fuzzy-search": "^3.2.1",
    "hotkeys-js": "^3.13.3",
    "immer": "10.0.3",
    "react": ">=18",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-intl": "^6.5.5",
    "react-redux": "^9.0.4",
    "react-virtuoso": "^4.6.2",
    "redux-watch": "^1.2.0",
    "shortid": "^2.2.16",
    "tsdef": "^0.0.14",
    "tss-react": "^4.9.3"
  },
  "devDependencies": {
    "@mui/material": "^5.15.2",
    "tsup": "^8.0.1",
    "@size-limit/preset-small-lib": "^11.0.1",
    "@types/classnames": "^2.3.0",
    "@types/fuzzy-search": "^2.1.5",
    "@types/memoizee": "^0.4.11",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/react-redux": "^7.1.33",
    "@types/redux-watch": "^1.1.2",
    "@types/shortid": "^0.0.32",
    "@vitejs/plugin-react": "^4.2.1",
    "prettier": "^3.1.1",
    "process": "^0.11.10",
    "react-is": "^18.2.0",
    "size-limit": "^11.0.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  },
  "homepage": "https://chonky.io/",
  "author": {
    "name": "Timur Kuzhagaliyev",
    "email": "tim.kuzh@gmail.com",
    "url": "https://foxypanda.me/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimboKZ/Chonky.git"
  },
  "bugs": {
    "url": "https://github.com/TimboKZ/Chonky/issues"
  },
  "keywords": [
    "file",
    "chonky",
    "file-dialog",
    "file-browser",
    "react-component"
  ]
}
