{
  "name": "@bhunter179/chonky-icon-fontawesome",
  "version": "0.2.9",
  "description": "FontAwesome icon component for Chonky file browser",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/chonky-icon-fontawesome.esm.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "dts build --format esm",
    "release": "npm publish --access=public",
    "patch": "npm --no-git-tag-version version patch",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "format": "prettier --config ../../.prettierrc.js --ignore-path ../../.prettierignore . --write",
    "format-check": "prettier --config ../../.prettierrc.js --ignore-path ../../.prettierignore --check ."
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "useTabs": false,
    "printWidth": 88
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@bhunter179/chonky": "latest",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "dts-cli": "^2.0.3",
    "husky": "^8.0.3",
    "size-limit": "^8.2.4",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3"
  },
  "homepage": "https://chonky.io/",
  "author": {
    "name": "Timur Kuzhagaliyev",
    "email": "tim.kuzh@gmail.com",
    "url": "https://foxypanda.me/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimboKZ/Chonky.git"
  },
  "bugs": {
    "url": "https://github.com/TimboKZ/Chonky/issues"
  },
  "keywords": [
    "file",
    "chonky",
    "file-dialog",
    "file-browser",
    "react-component"
  ]
}
