{
  "name": "@bhunter179/chonky",
  "version": "0.2.33",
  "description": "A File Browser component for React",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/chonky.esm.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "dts build --format esm",
    "release": "npm publish --access=public",
    "patch": "npm --no-git-tag-version version patch",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "format": "prettier --config ../../.prettierrc.js --ignore-path ../../.prettierignore . --write",
    "format-check": "prettier --config ../../.prettierrc.js --ignore-path ../../.prettierignore --check ."
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "useTabs": false,
    "printWidth": 88
  },
  "dependencies": {
    "@bhunter179/react-material-you-theme": "^0.1.18",
    "@emotion/css": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/material": "^5.13.5",
    "@reduxjs/toolkit": "^1.9.5",
    "deepmerge": "^4.3.1",
    "exact-trie": "^1.0.13",
    "fast-sort": "^3.4.0",
    "filesize": "^10.0.7",
    "fuzzy-search": "^3.2.1",
    "hotkeys-js": "^3.10.2",
    "immer": "10.0.2",
    "react": ">=16",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-intl": "^6.4.4",
    "react-redux": "^8.1.0",
    "react-virtuoso": "^4.3.11",
    "redux-watch": "^1.2.0",
    "shortid": "^2.2.16",
    "tsdef": "^0.0.14",
    "tss-react": "^4.8.6",
    "use-container-queries": "^1.0.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@testing-library/react": "^14.0.0",
    "@types/classnames": "^2.3.0",
    "@types/fuzzy-search": "^2.1.2",
    "@types/memoizee": "^0.4.8",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/react-redux": "^7.1.25",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@types/react-window-infinite-loader": "^1.0.6",
    "@types/redux-watch": "^1.1.0",
    "@types/shortid": "^0.0.29",
    "dts-cli": "^2.0.3",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "process": "^0.11.10",
    "react-is": "^18.2.0",
    "size-limit": "^8.2.4",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3"
  },
  "homepage": "https://chonky.io/",
  "author": {
    "name": "Timur Kuzhagaliyev",
    "email": "tim.kuzh@gmail.com",
    "url": "https://foxypanda.me/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimboKZ/Chonky.git"
  },
  "bugs": {
    "url": "https://github.com/TimboKZ/Chonky/issues"
  },
  "keywords": [
    "file",
    "chonky",
    "file-dialog",
    "file-browser",
    "react-component"
  ]
}
